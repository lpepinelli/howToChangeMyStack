<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - frontend</title>
        <!-- BEGIN VENDOR CSS-->
        <link rel="stylesheet" type="text/css" href="~/lib/app-assets/css/bootstrap.css">
        <!-- font icons-->
        <link rel="stylesheet" type="text/css" href="~/lib/app-assets/fonts/icomoon.css">
        <link rel="stylesheet" type="text/css" href="~/lib/app-assets/fonts/flag-icon-css/css/flag-icon.min.css">
        <link rel="stylesheet" type="text/css" href="~/lib/app-assets/vendors/css/extensions/pace.css">
        <!-- END VENDOR CSS-->
        <!-- BEGIN ROBUST CSS-->
        <link rel="stylesheet" type="text/css" href="~/lib/app-assets/css/bootstrap-extended.css">
        <link rel="stylesheet" type="text/css" href="~/lib/app-assets/css/app.css">
        <link rel="stylesheet" type="text/css" href="~/lib/app-assets/css/colors.css">
        <!-- END ROBUST CSS-->
        <!-- BEGIN Page Level CSS-->
        <link rel="stylesheet" type="text/css" href="~/lib/app-assets/css/core/menu/menu-types/vertical-menu.css">
        <link rel="stylesheet" type="text/css" href="~/lib/app-assets/css/core/menu/menu-types/vertical-overlay-menu.css">
        <link rel="stylesheet" type="text/css" href="~/lib/app-assets/css/core/colors/palette-gradient.css">
        <!-- END Page Level CSS-->
    <!--<link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">-->
    <link rel="stylesheet" href="~/css/main.css">
</head>
<body class="bg-light bg-gradient">


    <!-- navbar-fixed-top-->
    <nav class="header-navbar navbar navbar-with-menu navbar-fixed-top navbar-semi-dark navbar-shadow no-print reset">
        <div class="navbar-wrapper">
            <div class="navbar-container content container-fluid">
                <div class="navbar-toggleable-sm">
                    <ul class="nav navbar-nav">
                        <li class="nav-item"><a href="/" class="nav-link"><i class="icon-books"> Livros</i></a></li>
                        <li class="nav-item"><a href="/Genre" class="nav-link"><i class="icon-grid"> Gêneros</i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>

    <div class="app-content content container-fluid">
        <div class="content-wrapper reset leftprint mt-4">
            @RenderBody()
            <hr />
        </div>
    </div>

    <!-- BEGIN VENDOR JS-->
    <script src="~/lib/app-assets/js/core/libraries/jquery.min.js" type="text/javascript"></script>
    <script src="~/lib/app-assets/vendors/js/ui/tether.min.js" type="text/javascript"></script>
    <script src="~/lib/app-assets/js/core/libraries/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/lib/app-assets/vendors/js/ui/perfect-scrollbar.jquery.min.js" type="text/javascript"></script>
    <script src="~/lib/app-assets/vendors/js/ui/unison.min.js" type="text/javascript"></script>
    <script src="~/lib/app-assets/vendors/js/ui/blockUI.min.js" type="text/javascript"></script>
    <script src="~/lib/app-assets/vendors/js/ui/jquery.matchHeight-min.js" type="text/javascript"></script>
    <script src="~/lib/app-assets/vendors/js/ui/screenfull.min.js" type="text/javascript"></script>
    <script src="~/lib/app-assets/vendors/js/extensions/pace.min.js" type="text/javascript"></script>
    <script src="~/lib/app-assets/js/core/app-menu.js" type="text/javascript"></script>
    <script src="~/lib/app-assets/js/core/app.js" type="text/javascript"></script>
    <script src="~/lib/app-assets/js/scripts/pages/dashboard-lite.js" type="text/javascript"></script>

    <script defer="" src="https://unpkg.com/imask"></script>
    <script src="~/js/main.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
